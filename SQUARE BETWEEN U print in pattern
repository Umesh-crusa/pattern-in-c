# pattern-in-c

#include<stdio.h>
//umeshcoder
int main(){
int i,j,k=0;
for(i=1;i<=9;i++)
{
    i<=5?k++:k--;
    for(j=1;j<=9;j++)
    if(i==1||j==1||j==2||i==2||i==3||j==3||j==4||i==4||j==6||i==6||i==7||j==7||j==8||i==8||i==9||j==9)
    {
        printf("*");
    }
    else 
    {
    printf("U");
    }
    printf("\n");
}
return 0;
}
